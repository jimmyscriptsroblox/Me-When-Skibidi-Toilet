local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local Chat = game:GetService("Chat")

local player = Players.LocalPlayer
if not player then return end

local character = player.Character or player.CharacterAdded:Wait()
local head = character:WaitForChild("Head")
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

local sound1 = Instance.new("Sound")
sound1.SoundId = "rbxassetid://9118841731"
sound1.Volume = 8
sound1.Looped = true
sound1.Parent = humanoidRootPart
sound1:Play()

local bodyVelocity = Instance.new("BodyVelocity")
bodyVelocity.Velocity = Vector3.new(0, 1000, 0)
bodyVelocity.MaxForce = Vector3.new(0, math.huge, 0)
bodyVelocity.Parent = humanoidRootPart

local bodyAngularVelocity = Instance.new("BodyAngularVelocity")
bodyAngularVelocity.AngularVelocity = Vector3.new(1000, 1000, 1000)
bodyAngularVelocity.MaxTorque = Vector3.new(1e6, 1e6, 1e6)
bodyAngularVelocity.P = 1250
bodyAngularVelocity.Parent = humanoidRootPart

local segments = {
    "Among Us, Skibidi Toilet at 3 AM, gyatt sigma rizz?! Skibididi Toilet sigma theta gigachad?!?!?!? Ohio Final Boss vs. Shaggy Ultra Instinctâ€”who wins?! ğŸš¨ğŸš€ Drip Goku just pulled up with the infinite rizzler mindset, but Walter White in the back cooking up that sigma grindset formula?! ğŸ˜±ğŸ”¥ NPCs glitching out while Quandale Dingle narrates the loreâ€”ARE WE EVEN REAL?! ğŸ’€ğŸ’€ğŸ’€ Bro just hit the gritty in the backrooms while Sigma Chad Patrick Bateman drops a 1000 IQ playâ€¦ THIS CANâ€™T BE HAPPENING! ğŸš¨ğŸ˜‚",
    "BUT WAITâ€”Shrek just ascended to his ultimate ogrelord form, unlocking 100% of his meme potential?! ğŸ˜³ğŸ”¥ Meanwhile, Morbius is morb-ing through dimensions, powered by the Elden Ring rizz buff, while Big Smoke finally orders the correct meal?! ğŸ”ğŸ’€ Bro, the Rizzler just pulled up in a Ligma-powered Tesla with Elon Musk and Peter Griffin in the passenger seat discussing intergalactic grindset strategies?! ğŸš€ğŸ’° Meanwhile, Sonic dashes through Ohio, but he just got clipped into the Backrooms and now the Rock is raising his eyebrow at the camera?! ğŸ˜±ğŸ’€",
    "NOT TO MENTIONâ€¦ John Pork is calling, but the phone is connected to an AI-generated Drake song produced by Walter White's alter ego, DJ Heisenberg?! ğŸ¶ğŸ˜µâ€ğŸ’« And bro, WHY IS SHADOW THE HEDGEHOG STANDING IN THE CORNER SAYING â€œITâ€™S MORBINâ€™ TIMEâ€ IN REVERSE WHILE THE FNAF AMBIENCE PLAYS?! ğŸ¤ğŸ˜¨ THE UNIVERSE CANâ€™T HANDLE THIS MUCH GYATTIFIED SIGMA LORE. ğŸš€ğŸ”¥ğŸ’€"
